{
  "app": "npx ts-node --prefer-ts-exts bin/single-ec2.ts",
  "context": {
    "aws-cdk:enableDiffNoFail": "true",
    "aws-cdk:enableStackNameDuplicates": "true",
    "aws-cdk:stackRelativeExports": "true",
    "aws-cdk:usagePlanKeyOrderInsensitiveId": true,
    "aws-cdk:dockerIgnoreSupport": true,
    "aws-cdk:parseOwnedSecretName": true,
    "aws-cdk:defaultKeyPolicies": true,
    "aws-cdk:grantWriteWithoutAcl": true,
    "aws-cdk:removeDefaultDesiredCount": true,
    "aws-cdk:lowercaseDbIdentifier": true,
    "aws-cdk:defaultEncryptionAtRest": true,
    "aws-cdk:recognizeVersionProps": true,
    "aws-cdk:defaultSecurityPolicyTLSv1.2_2021": true
  }
}
